<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>controller.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet1</servlet-name>
        <servlet-class>controller.statisticsAndReporting.NewServlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CurrentInventoryServlet</servlet-name>
        <servlet-class>controller.statisticsAndReporting.CurrentInventoryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateImportReceiptServlet</servlet-name>
        <servlet-class>controller.warehouseImportMgt.CreateImportReceiptServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductLookupServlet</servlet-name>
        <servlet-class>controller.warehouseImportMgt.ProductLookupServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CategoryLookupServlet</servlet-name>
        <servlet-class>controller.warehouseImportMgt.CategoryLookupServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImportExportStatsServlet</servlet-name>
        <servlet-class>controller.statisticsAndReporting.ImportExportStatsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImportReceiptListServlet</servlet-name>
        <servlet-class>controller.warehouseImportMgt.ImportReceiptListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImportReceiptDetailServlet</servlet-name>
        <servlet-class>controller.warehouseImportMgt.ImportReceiptDetailServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/new</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet1</servlet-name>
        <url-pattern>/new1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CurrentInventoryServlet</servlet-name>
        <url-pattern>/current-inventory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateImportReceiptServlet</servlet-name>
        <url-pattern>/createImportReceipt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductLookupServlet</servlet-name>
        <url-pattern>/api/product-lookup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CategoryLookupServlet</servlet-name>
        <url-pattern>/api/category-lookup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImportExportStatsServlet</servlet-name>
        <url-pattern>/import-export-stats</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImportReceiptListServlet</servlet-name>
        <url-pattern>/warehouse-import-mgt/import-receipt-list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImportReceiptDetailServlet</servlet-name>
        <url-pattern>/warehouse-import-mgt/import-receipt-detail</url-pattern>
    </servlet-mapping>
</web-app>
